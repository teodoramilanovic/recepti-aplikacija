package local.rps.korisnik;
import java.util.ArrayList;

import local.rps.recept.Recept;

public class KorisnikDetails {
	private int id;
	private String ime, prezime;
	private String pol;
	private String korisnickoIme, email, lozinka;
	private String avatar;
	private int brojPratioci;
	private int brojPrati;
	private List<Recept> recepti;
	
	public KorisnikDetails(Korisnik k) {
		super();
		this.id = k.getId();
		this.ime = k.getIme();
		this.prezime = k.getPrezime();
		this.pol = k.getPol();
		this.korisnickoIme = k.getKorisnickoIme();
		this.email = k.getEmail();
		this.lozinka = k.getLozinka();
		this.avatar = k.getAvatar();
	}

	public int getId() {
		return id;
	}

	public void setId(int id) {
		this.id = id;
	}

	public String getIme() {
		return ime;
	}

	public void setIme(String ime) {
		this.ime = ime;
	}

	public String getPrezime() {
		return prezime;
	}

	public void setPrezime(String prezime) {
		this.prezime = prezime;
	}

	public String getPol() {
		return pol;
	}

	public void setPol(String pol) {
		this.pol = pol;
	}

	public String getKorisnickoIme() {
		return korisnickoIme;
	}

	public void setKorisnickoIme(String korisnickoIme) {
		this.korisnickoIme = korisnickoIme;
	}

	public String getEmail() {
		return email;
	}

	public void setEmail(String email) {
		this.email = email;
	}

	public String getLozinka() {
		return lozinka;
	}

	public void setLozinka(String lozinka) {
		this.lozinka = lozinka;
	}

	public String getAvatar() {
		return avatar;
	}

	public void setAvatar(String avatar) {
		this.avatar = avatar;
	}

	public int getBrojPratioci() {
		return brojPratioci;
	}

	public void setBrojPratioci(int brojPratioci) {
		this.brojPratioci = brojPratioci;
	}

	public int getBrojPrati() {
		return brojPrati;
	}

	public void setBrojPrati(int brojPrati) {
		this.brojPrati = brojPrati;
	}

	public ArrayList<Recept> getRecepti() {
		return recepti;
	}

	public void setRecepti(ArrayList<Recept> recepti) {
		this.recepti = recepti;
	}
	
	
}
